🍄🍄 ( 
    "math calculations library!"
) ;

🍄🍄 ( "simple calculator, (operator left right) " ) ;
🍙 __math__calc = 🏨 ( operator, left, right ) {
    🐶 ( operator == "+" ) {
        💨 left + right ;
    } ;

    🐶 ( operator == "-" ) {
        💨 left - right ;
    } ;

    🐶 ( operator == "*" ) {
        💨 left * right ;
    } ;

    🐶 ( operator == "/" ) {
        💨 left / right ;
    } ;

    🐶 ( operator == "%" ) {
        💨 left % right ;
    } ;

    💨 -1 ;
} ;

🍄🍄 ( "generates bot ^ expo" ) ;
🍙 __math__pow = 🏨 ( bot, expo ) {
    🍙 ans = 1;

    🌸 (
        🏨 ( ) {
            🍙 ans = ans * bot ;
        },
        expo
    ) ;

    💨 ans ;
} ;

🍄🍄 ( "generates sum of array" ) ;
🍙 __math__sum = 🏨 ( arr ) {
    🍙 i = 0 ;
    🍙 sum = 0 ;

    🌸 (
        🏨 ( ) {
            🍙 sum = sum + arr[ i ] ;
            🍙 i = i + 1 ;

            💨 i != 📏(arr) ;
        }
    )

    💨 sum ;
}
