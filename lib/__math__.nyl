
🍄🍄 ( "simple calculator, (operator left right) " ) ;
🍙 __math__calc = 🏨 ( operator, left, right ) {
    🐶 ( operator == "+" ) {
        💨 left + right ;
    } ;

    🐶 ( operator == "-" ) {
        💨 left - right ;
    } ;

    🐶 ( operator == "*" ) {
        💨 left * right ;
    } ;

    🐶 ( operator == "/" ) {
        💨 left / right ;
    } ;

    💨 -1 ;
} ;